{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "indent": ["error", 2],
    "no-console": 2,
    "no-alert": 2,
    "no-debugger": 2
  },
  "plugins": [
    "testcafe"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:testcafe/recommended"
  ]
}